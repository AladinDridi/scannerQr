<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.aladin_bureau.kademiatn">

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.CAMERA" />
    <uses-permission android:name="android.permission.VIBRATE" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/iconappli"
        android:label="@string/app_name"
        android:roundIcon="@drawable/iconappli"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".ScreenSplash">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".MenyNavigation"
            android:configChanges="orientation|screenSize"
            android:label="@string/title_activity_meny_navigation" />
        <activity android:name=".ContactActivity" />
        <activity android:name=".qrscannerActivity" />
        <activity android:name=".Contact"></activity>
    </application>

</manifest>